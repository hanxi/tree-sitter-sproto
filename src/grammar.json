{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sproto",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "type_definition"
          },
          {
            "type": "SYMBOL",
            "name": "protocol_definition"
          }
        ]
      }
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    },
    "type_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "field_definition"
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "field_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "SYMBOL",
            "name": "integer"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "is_array",
              "content": {
                "type": "STRING",
                "value": "*"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "map_key_type",
              "content": {
                "type": "SYMBOL",
                "name": "map_specifier"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "map_specifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "protocol_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "tag",
          "content": {
            "type": "SYMBOL",
            "name": "integer"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "request_block"
                },
                {
                  "type": "SYMBOL",
                  "name": "response_block"
                },
                {
                  "type": "SYMBOL",
                  "name": "type_definition"
                }
              ]
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "request_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "request"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "{"
                },
                {
                  "type": "FIELD",
                  "name": "body",
                  "content": {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "field_definition"
                    }
                  }
                },
                {
                  "type": "STRING",
                  "value": "}"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "response_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "response"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "{"
                },
                {
                  "type": "FIELD",
                  "name": "body",
                  "content": {
                    "type": "REPEAT",
                    "content": {
                      "type": "SYMBOL",
                      "name": "field_definition"
                    }
                  }
                },
                {
                  "type": "STRING",
                  "value": "}"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}