{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sproto",
      "camelcase": "sproto",
      "title": "sproto",
      "scope": "source.sproto",
      "file-types": [
        "sproto"
      ],
      "injection-regex": "^sproto$",
      "class-name": "TreeSitterSproto"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree sitter for sproto",
    "authors": [
      {
        "name": "hanxi",
        "email": "im.hanxi@gmail.com",
        "url": "https://github.com/hanxi"
      }
    ],
    "links": {
      "repository": "https://github.com/hanxi/tree-sitter-sproto"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}